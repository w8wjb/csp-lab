{
  "name": "CSP Lab",
  "version": "0.1",
  "manifest_version": 3,
  "description": "Develop Content Security Policies (CSP) for your website",
  "icons": {
    "16": "images/icon16.png",
    "32": "images/icon32.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "background": {
    "service_worker": "service_worker.js"
  },
  "devtools_page": "devtools.html",
  "options_page": "options.html",
  "host_permissions": [
    "*://*/*"
  ],
  "permissions": ["declarativeNetRequest", "declarativeNetRequestFeedback", "webNavigation", "webRequest", "storage"]
}
